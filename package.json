{
  "name": "messageboard-backend-express",
  "version": "0.0.0",
  "description": "Simple node.js demonstration with Postgres database",
  "private": true,
  "scripts": {
    "start_nodemon": "nodemon app.js",
    "start": "set DEBUG= app & nodemon app.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.2",
    "pg": "^7.12.1",
    "pug": "2.0.0-beta11"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": 2500,
    "env": {
      "NODE_ENV": "development",
      "PORT": 3000
    }
  }
}
